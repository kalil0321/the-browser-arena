# Convex Configuration
CONVEX_URL=https://your-deployment.convex.cloud

# Anchor Browser API Key
ANCHOR_API_KEY=your_anchor_api_key_here

# Notte API Key (optional - enables the Notte agent)
NOTTE_API_KEY=your_notte_api_key_here

# Agent Server API Key (required for authenticating requests to the Python server)
# Generate a strong random key: openssl rand -hex 32
AGENT_SERVER_API_KEY=your_agent_server_api_key_here
# Same as agent key but for interacting with convex backend
BACKEND_API_KEY=...

# Browser-Use API Key (optional - uses their hosted model)
BROWSER_USE_API_KEY=your_browser_use_api_key_here

# Optional: LLM Provider API Keys (if not using Browser-Use hosted model)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GOOGLE_API_KEY=your_google_api_key_here

